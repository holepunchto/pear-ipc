{
  "name": "pear-ipc",
  "version": "6.9.0",
  "description": "IPC for Pear",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:node && npm run test:bare",
    "format": "prettier --write .",
    "lint": "prettier --check . && lunte",
    "test:node": "brittle-node --coverage test.js",
    "test:bare": "brittle-bare --coverage test.js"
  },
  "files": [
    "api.js",
    "index.js",
    "methods.js",
    "server.js",
    "client.js",
    "constants.js"
  ],
  "keywords": [],
  "author": "Holepunch",
  "license": "Apache-2.0",
  "dependencies": {
    "bare-fs": "^4.0.1",
    "bare-os": "^3.2.0",
    "bare-path": "^3.0.0",
    "bare-pipe": "^4.0.5",
    "framed-stream": "^1.0.1",
    "fs-native-extensions": "^1.2.7",
    "fs-native-lock": "^1.1.0",
    "ready-resource": "^1.0.3",
    "streamx": "^2.16.1",
    "tiny-buffer-rpc": "^2.2.0",
    "which-runtime": "^1.2.0"
  },
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "os": {
      "bare": "bare-os",
      "default": "os"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    },
    "net": {
      "bare": "bare-pipe",
      "default": "net"
    }
  },
  "devDependencies": {
    "brittle": "^3.4.0",
    "lunte": "^1.3.0",
    "prettier": "^3.6.2",
    "prettier-config-holepunch": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/pear-ipc.git"
  },
  "bugs": {
    "url": "https://github.com/holepunchto/pear-ipc/issues"
  },
  "homepage": "https://github.com/holepunchto/pear-ipc#readme"
}
